local keyname  = ("Data_"..plr.UserId..game.PlaceId..".lua")
if not isfile(keyname) then
  writefile(keyname,"")
end

local function getval(key)
  local data = string.split(readfile(keyname),",")
  found = false
  for i,v in pairs(data) do
    if v == key then
      found = data[i+1]
    end
  end
  if found then
    return(found)
  else
    return nil
  end
end

local function editval(k1,k2)
    
  local comma = false
  local data = string.split(readfile(keyname),",")
  local edit = nil
  local newdata = ""
  for i,v in pairs(data) do
    if v == edit then v = k2 
        edit = nil end
    if v == k1 then  edit = data[i+1]  end
    
    if comma then
      newdata = newdata..","..v
    else
      newdata = newdata..v
    end
    comma = true
  end
  writefile(keyname,tostring(newdata))
end

local function makeval(nd1,nd2)
  writefile(keyname,readfile(keyname)..","..nd1..","..nd2)
end


local eggs = {}
local eggloc = game:GetService("ReplicatedStorage").Framework2.Modules.Shared.Internal["1 | Directory"].Eggs
for i,v in pairs(eggloc:GetChildren()) do
