local function checkrev(String)
   
 String = string.split(String," ","")

 for i,v in pairs (String) do
     if scrolly:FindFirstChild(v) then
         found =  scrolly:FindFirstChild(v)
         if found.TextColor3 == Color3.fromRGB(255, 255, 255) then
             found.TextColor3 = Color3.fromRGB(255, 0, 0) else
                 found.TextColor3 = Color3.fromRGB(255, 255, 255)
         end
         if scrolly:FindFirstChild("un"..found.Name) then
             scrolly:FindFirstChild("un"..found.Name).TextColor3 = found.TextColor3
             end 
     end
 end
end



 for i,v in pairs(scrolly:GetChildren()) do
     if v:IsA("TextButton") then
         v.MouseButton1Down:Connect(function()
         editval(v.Text,"true") 
   
     end)
     end
     end
         
         
prefix = ":"
