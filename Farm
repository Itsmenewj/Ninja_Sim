  local function tweener(meframe,too,timer)
      game:GetService("TweenService"):Create(meframe,
      TweenInfo.new(timer, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
      {CFrame = too},true):Play()
   end
  local function drop(Target)

            if game.Players:FindFirstChild(Target.Name) then

               local char = plr.Character

               local tool = plr.Character:FindFirstChildOfClass("Tool") or nil



               db = true
               tool.Handle.Touched:Connect(function(t)

               if t == Target.Character.HumanoidRootPart and db == true then

                  local hum = plr.Character:FindFirstChildOfClass("Humanoid")
                  local hrp = char.HumanoidRootPart
                  hum.Name = "1"
                  local newHum = hum:Clone()
                  newHum.Parent = char
                  newHum.Name = "Humanoid"
                  hum:Destroy()

                  workspace.CurrentCamera.CameraSubject = char
                  newHum.DisplayDistanceType = "None"
                  db = false
               end
               end)


               tweener(char.HumanoidRootPart,Target.Character.HumanoidRootPart.CFrame + Target.Character.HumanoidRootPart.CFrame.lookVector * -2,0.1)
               tweener(tool.Handle,Target.Character.HumanoidRootPart.CFrame ,0.1)

               repeat wait(.01)
                  tool.Handle.CFrame = Target.Character.HumanoidRootPart.CFrame
               until not char or tool.Parent == Target.Character

            end
  end
     



