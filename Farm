 local function notify(Title, Text, Button1, Button2, Callback)
         if ScreenGui.Parent ~= nil then
            if Button1 then
               Duration = 500
            else
               Duration = 3
            end
            Callback = Callback or function()
         end
         local Bindable = Instance.new("BindableFunction")
         Bindable.OnInvoke = Callback

         game:GetService("StarterGui"):SetCore(
         "SendNotification",
         { Title = Title,
            Text = Text,
            Duration = Duration,
            Button1 = Button1,
            Button2 = Button2,
            Callback = Bindable
         }
         )
      end
 end  
local allow = false
local function permit()
    notify("WARNING!","This may result in PERMANENT LOSS OF PURCHASED PETS","I Understand","Cancel",function(Answer)
               if Answer == "I Understand" then
                 allow = true
               end
               end)
end

