            local Tab = game.Players.LocalPlayer.PlayerGui.Main.Windows.Pets.PetsList:GetChildren()
            for i,v in pairs(game.Players.LocalPlayer.PlayerGui.Main.Windows.Pets.PetsList:GetChildren()) do

               if v:IsA("Frame") and game.Players.LocalPlayer.PlayerGui.Main.Windows.Pets.PetsList:FindFirstChild(v.Name) then
                  table.insert(items,v)
               end
            end
            local Event3 = game:GetService("ReplicatedStorage").Framework2.Modules.Shared.Internal.Modules["2 | Network"].Remotes.craftpets
            for i,v in pairs(items) do
               pcall(function()
               local num = 0
               local tab1 = {
                  [1] = {};
               }
               for i,f in pairs(game.Players.LocalPlayer.PlayerGui.Main.Windows.Pets.PetsList:GetChildren()) do
                  if  f:IsA("Frame") then

                     if f.Bg.Icon.Image == v.Bg.Icon.Image  and f.Bg.PetMultiplier.Text == v.Bg.PetMultiplier.Text then
                        tt =  game.Players.LocalPlayer.PlayerGui.Main.Windows.Pets.PetsList
                        if tt:FindFirstChild(v.Name) and tt:FindFirstChild(f.Name) then
                           g = f.Name
                           tab1[1][g] = true
                           num = num + 1
                           if num == 3 then
                              Event3:InvokeServer(tab1)
                           end
                        end
                     end

                  end

               end
               end)
            end
         end
             end
         end)
game.Players.PlayerRemoving:Connect(function(p)
if p == plr then
  if getval("date") then
      editval("date",os.date ("!%c"))
      else
          makeval("date",os.date ("!%c"))
  end
 editval("speedfly ",speedfly)
 end
end)


