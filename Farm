local green = Color3.fromRGB(57, 255, 57)
         local blue = Color3.fromRGB(57, 57, 255)
         local grey = Color3.fromRGB(255, 255, 255)
         local red = Color3.fromRGB(255, 57, 57)
         


local b3db = false
local function giver(button1,vict)
   
        local Sword1 = plr.Character:FindFirstChildOfClass("Tool")
         if not  Sword1 then
            notify("No Tool Equiped","Equip "..plr.Backpack:FindFirstChildOfClass("Tool").Name,"Yes","No",function(Answer)
            if Answer == "Yes" then
               plr.Backpack:FindFirstChildOfClass("Tool").Parent = plr.Character
               Sword1 = plr.Character:FindFirstChildOfClass("Tool")
            else
              button1.TextColor3 = Color3.fromRGB(255, 0, 0)
               wait(1) button1.TextColor3 = Color3.fromRGB(255, 255, 255)
               Sword1 = nil
            end
            end)
         end
         repeat wait() until Sword1
            Sword1 = Sword1.Name or nil

            if b3db == false then b3db = true
             button1.TextColor3 = green
            game.Players[vict].Character.ChildAdded:Connect(function(child)
            if child.Name == Sword1 then
               button1.TextColor3 = blue
               wait(1)
               if b3db then button1.TextColor3 = green else button1.TextColor3 = grey end
            end
            if child.Name ~= Sword1 and b3db == true
            and child:IsA("Tool") then
               if not game.Players[vict].Backpack:FindFirstChild(Sword1) then
                   
                  drop(game.Players[vict])
               end
            end
end)
 drop(game.Players[vict])
         else b3db = false
            button1.TextColor3 = grey
         end

         end

               



