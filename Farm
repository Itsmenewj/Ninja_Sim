TextBox1.Changed:Connect(function(f)
     if f == "Text" then
      for i,v in pairs (F2:GetChildren()) do
          if v:IsA("TextButton") then
         if v.Text:lower():sub(1, #TextBox1.Text) == TextBox1.Text:lower() or
         v.Text:sub(1, #TextBox1.Text) == TextBox1.Text then
            F2:FindFirstChild(v.Name).Visible = true
         else F2:FindFirstChild(v.Name).Visible = false
         end
          end
      end
end
end)
